IF
	Global("Begin","MYAREA",0)
THEN
	RESPONSE #100
	SetInterrupt(FALSE)
	ActionOverride("C0Auin3",MakeGlobalOverride())
	ActionOverride("C0AUIN3",SetGlobal("Standby","LOCALS",1))
	ChangeEnemyAlly("C0auin3",NEUTRAL)
	AmbientActivate("Portal1",FALSE)
	AmbientActivate("Portal2",FALSE)
	AmbientActivate("Portal3",FALSE)
	AmbientActivate("Portal4",FALSE)
	AmbientActivate("Portal5",FALSE)
	SetGlobal("Begin","MYAREA",1)
	SetGlobal("C0AutoCreated", "GLOBAL", 1)
	SetInterrupt(TRUE)
END

IF
    !TimeOfDay(MORNING)
	!TimeOfDay(NIGHT)
	IsActive("c0akoko2")
THEN
    RESPONSE #100
