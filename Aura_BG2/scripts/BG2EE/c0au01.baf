IF
	Global("C0AuraBG2Quest2","GLOBAL",4)
  InParty("C0Aura")
  !Dead("C0Aura")
  PartyHasItem("C0AMISC1")
  PartyHasItem("C0AMISC2")
  PartyHasItem("C0AMISC3")
  PartyHasItem("C0AMISC4")
THEN
	RESPONSE #100
		SetGlobal("C0AuraBG2Quest2","GLOBAL",5)
		StartCutSceneMode()
    ContainerEnable("Generator1",TRUE)
    ContainerEnable("Generator2",TRUE)
    ContainerEnable("Generator3",TRUE)
    ContainerEnable("Generator4",TRUE)
    ActionOverride("C0Aura",MoveToPoint([433.225])
    ActionOverride("C0Aura",SmallWait(1))
    ActionOverride("C0Aura",Face(E))
    ActionOverride("C0Aura",Wait(1))
    ActionOverride("C0Aura",MoveToPoint([505.303])
    ActionOverride("C0Aura",SmallWait(1))
    ActionOverride("C0Aura",Face(NE))
    ActionOverride("C0Aura",Wait(1))
    ActionOverride("C0Aura",MoveToPoint([587.290])
    ActionOverride("C0Aura",SmallWait(1))
    ActionOverride("C0Aura",Face(W))
    ActionOverride("C0Aura",Wait(4))
    ActionOverride("C0Aura",EndCutSceneMode())
    ActionOverride("C0Aura",StartDialogNoSet(Player1))
END

